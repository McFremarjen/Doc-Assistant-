<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/lightblue"
    tools:context="com.mcfremarjentechnologies.updated.drass.start"
    tools:layout_editor_absoluteY="25dp">


    <ru.dimorinny.floatingtextbutton.FloatingTextButton
        android:id="@+id/Createbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="220dp"
        app:floating_background_color="@color/colorPrimary"
        app:floating_left_icon="@drawable/ic_lock_open_black_24dp"
        app:floating_title="@string/test"
        app:floating_title_color="@android:color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <ru.dimorinny.floatingtextbutton.FloatingTextButton
        android:id="@+id/lognbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        app:floating_background_color="@color/colorPrimary"
        app:floating_left_icon="@drawable/ic_createblack_24dp"
        app:floating_title="@string/login"
        app:floating_title_color="@android:color/white"
        app:layout_constraintBottom_toTopOf="@+id/Createbtn"
        app:layout_constraintEnd_toEndOf="@+id/Createbtn"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Createbtn"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.962" />


</android.support.constraint.ConstraintLayout>
